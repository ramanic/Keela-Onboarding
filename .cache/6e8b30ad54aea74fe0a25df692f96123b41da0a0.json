err);\n        } else {\n          this.organizations = res.filter((organization) => {\n            return organization.owner === Meteor.userId();\n          });\n        }\n      });\n    },\n    addUser() {\n      if ((this.name != '') & (this.password != '')) {\n        Meteor.call(\n          'users.insert',\n          this.name,\n          this.password,\n          this.role,\n          this.organization,\n          (err, res) => {\n            if (err) {\n              console.log(err);\n            } else {\n              this.getUsers();\n              console.log(res);\n            }\n          }\n        );\n        this.name = '';\n        this.password = '';\n        this.role = '';\n        this.organization = '';\n      }\n    },\n    getUsers() {\n      Meteor.call('users.view', (err, res) => {\n        if (err) {\n          console.log(err);\n        } else {\n          this.users = res;\n          console.log('users', this.users);\n        }\n      });\n    },\n    updateUser() {\n      Meteor.call('users.update', this.userToUpdate, (err, res) => {\n        if (err) {\n          console.log(err);\n        } else {\n          this.getUsers();\n          this.$modal.hide('user-update');\n          console.log(res);\n        }\n      });\n    },\n  },\n};\n"]},"sourceType":"module","hash":"6e8b30ad54aea74fe0a25df692f96123b41da0a0"}
