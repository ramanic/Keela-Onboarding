lections';\nimport { Meteor } from 'meteor/meteor';\n\nexport default {\n  data() {\n    return {\n      mUser: { _id: 1 },\n      name: '',\n      email: '',\n      organization: {},\n      contacts: [],\n      contactToUpdate: {\n        name: '',\n        email: '',\n      },\n    };\n  },\n  created() {\n    this.getContacts();\n    this.mUser = Meteor.users.findOne(\n      { _id: Meteor.user()._id },\n      { fields: { role: 1, organization: 1 } }\n    );\n    let org = OrganizationsCollection.find({\n      _id: this.mUser.organization,\n    }).fetch();\n    this.organization = org[0];\n    console.log('Org', this.organization);\n  },\n\n  methods: {\n    updateContactShow(data) {\n      this.$modal.show('contact-update');\n      this.userToUpdate = { ...data };\n    },\n\n    deleteContact(id) {\n      Meteor.call('contacts.delete', id, (err, res) => {\n        if (err) {\n          console.log(err);\n        } else {\n          console.log(res);\n          this.getContacts();\n        }\n      });\n    },\n\n    addContacts() {\n      if ((this.name != '') & (this.email != '')) {\n        Meteor.call('contacts.insert', this.name, this.email, (err, res) => {\n          if (err) {\n            console.log(err);\n          } else {\n            this.getContacts();\n            console.log(res);\n          }\n        });\n        this.name = '';\n        this.email = '';\n      }\n    },\n    getContacts() {\n      Meteor.call('contacts.view', (err, res) => {\n        if (err) {\n          console.log(err);\n        } else {\n          this.contacts = res;\n          console.log('contacts', this.contacts);\n        }\n      });\n    },\n    updateContacts() {\n      Meteor.call('contacts.update', this.contactsToUpdate, (err, res) => {\n        if (err) {\n          console.log(err);\n        } else {\n          this.getUsers();\n          this.$modal.hide('contact-update');\n          console.log(res);\n        }\n      });\n    },\n  },\n};\n"]},"sourceType":"mo